    "underscore": "^1.12.0"
  },
  "devDependencies": {
    "css-loader": "^5.0.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.6",
